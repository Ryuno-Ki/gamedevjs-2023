{
  "name": "gamedevjs-2023",
  "version": "0.1.0",
  "description": "Theme is TIME.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild app=src/index.js --bundle --minify --sourcemap=inline --outdir=dist",
    "build:dev": "esbuild app=src/index.js --bundle --outdir=dist",
    "clean": "rimraf ./dist && mkdir ./dist && touch ./dist/.gitkeep",
    "lint": "yarn lint:src && yarn lint:test",
    "lint:src": "standard src",
    "lint:test": "standard test --global beforeEach --global describe --global expect",
    "postinstall": "husky install",
    "postpack": "pinst --enable",
    "prebuild": "yarn clean && yarn test:ci",
    "prebuild:dev": "yarn clean",
    "prepack": "pinst --disable",
    "prepare": "husky install",
    "test": "mocha",
    "test:ci": "c8 mocha",
    "tsc": "tsc",
    "tsd": "tsd",
    "types": "tsc --noEmit false --emitDeclarationOnly true"
  },
  "repository": {
    "type": "git",
    "url": "ryunoki@jaenis.ch:ryuno-ki/gamedevjs-2023.git"
  },
  "keywords": [
    "gamedevjs",
    "game",
    "jam"
  ],
  "author": "Andr√© Jaenisch",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@commitlint/cli": "17.6.0",
    "@commitlint/config-conventional": "17.6.0",
    "c8": "7.13.0",
    "chai": "4.3.7",
    "esbuild": "0.17.16",
    "husky": "8.0.3",
    "jsdom": "21.1.1",
    "mocha": "10.2.0",
    "pinst": "3.0.0",
    "rimraf": "5.0.0",
    "standard": "17.0.0",
    "tsd": "0.28.1",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "socket.io": "4.6.1"
  }
}
